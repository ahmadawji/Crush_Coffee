<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.FD_CoffeeShop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Crush_Coffee">


        <activity
            android:name="com.example.FD_CoffeeShop.Create_Account"
            android:exported="true" />
        <activity
            android:name="com.example.FD_CoffeeShop.QRcodeActivity"
            android:exported="true" />
        <activity
            android:name="com.example.FD_CoffeeShop.Basket"
            android:exported="true" />
        <activity
            android:name="com.example.FD_CoffeeShop.OrderDetails"
            android:exported="true" />
        <activity
            android:name="com.example.FD_CoffeeShop.MenuActivity"
            android:exported="true" />
        <activity
            android:name="com.example.FD_CoffeeShop.loginActivity"
            android:exported="true" />
        <activity
            android:name="com.example.FD_CoffeeShop.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".Services.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>